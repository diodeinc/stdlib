hints = struct(
    near=lambda c1, c2: "near({},{})".format(c1, c2),
    decuples=lambda c: "decuples({})".format(c),
    rotate=lambda degrees: "rotate({})".format(degrees),
    right=lambda c: "right({})".format(c),
)


# Layout hints
def layout_hints(hints: list[str]):
    return add_property("layout_hints", hints)


# Schematic group
def schematic_group():
    return add_property("schematic_group", True)
