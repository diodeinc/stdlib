    type = "molex_nano_fit",
    symbol = Symbol(_symbol(variant)),
    footprint = File(_footprint(variant)),
    prefix = "J",
    pins = pins,
    properties = Properties(
        properties,
        {
            "value": _mpn(variant),
            "mpn": _mpn(variant),
        },
    ),
)
