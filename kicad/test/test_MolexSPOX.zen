load("../../properties.zen", "Layout")

MolexSPOX = Module("../MolexSPOX.zen")

for n, variant in enumerate(MolexSPOX.Variant):
    name = "MolexSPOX_" + variant.value + "_" + str(n)
    pins = {f"Pin_{i}": Net(f"Pin_{i}") for i in range(1, MolexSPOX.PinCount(variant) + 1)}
    MolexSPOX(name = name, variant = variant.value, **pins)

Layout(
    name = "preview",
    path = "build/MolexSPOX",
)