load("../../properties.zen", "Layout")

MolexNanoFit = Module("../MolexNanoFit.zen")

for n, variant in enumerate(MolexNanoFit.Variant):
    name = "MolexNanoFit_" + variant.value + "_" + str(n)
    pins = {f"Pin_{i}": Net(f"Pin_{i}") for i in range(1, MolexNanoFit.PinCount(variant) + 1)}
    MolexNanoFit(name = name, variant = variant.value, **pins)

Layout(
    name = "preview",
    path = "build/MolexNanoFit",
)