load("../../properties.zen", "Layout")

MolexPicoBlade = Module("../MolexPicoBlade.zen")

for n, variant in enumerate(MolexPicoBlade.Variant):
    name = "MolexPicoBlade_" + variant.value + "_" + str(n)
    pins = {f"Pin_{i}": Net(f"Pin_{i}") for i in range(1, MolexPicoBlade.PinCount(variant) + 1)}
    MolexPicoBlade(name = name, variant = variant.value, **pins)

