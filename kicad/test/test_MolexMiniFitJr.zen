load("../../properties.zen", "Layout")

MolexMiniFitJr = Module("../MolexMiniFitJr.zen")

for n, variant in enumerate(MolexMiniFitJr.Variant):
    name = "MolexMiniFitJr_" + variant.value + "_" + str(n)
    pins = {f"Pin_{i}": Net(f"Pin_{i}") for i in range(1, MolexMiniFitJr.PinCount(variant) + 1)}
    MolexMiniFitJr(name = name, variant = variant.value, **pins)

Layout(
