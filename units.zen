# Pure units
PI = 3.14159265358979323846

# Toleranced units
Time = builtin.Time
Current = builtin.Current
Voltage = builtin.Voltage
Capacitance = builtin.Capacitance
Resistance = builtin.Resistance
Inductance = builtin.Inductance
Frequency = builtin.Frequency
Temperature = builtin.Temperature
Charge = builtin.Charge
Power = builtin.Power
Energy = builtin.Energy
MagneticFlux = builtin.MagneticFlux
Conductance = builtin.Conductance

def unit(spec, type):
    return type(spec)
