"""Test for Spice model of Resistor."""

load("//amplifiers.zen", "sallen_key_lowpass", "voltage_follower")
load("//interfaces.zen", "Ground", "Opamp", "Power")

OperationalAmplifier = Module("//generics/OperationalAmplifier.zen")

VP = Power("VP")
VN = Power("VN")
INPUT = Net("INPUT")
PROBE = Net("PROBE")
GND = Ground()

OPAMP = sallen_key_lowpass(
    name="OPAMP",
    frequency="1MHz",
    GND=GND,
    IN=INPUT,
    OUT=PROBE,
)

OperationalAmplifier(
    name="OPAMP",
    VP=VP,
    VN=VN,
    OPA1=OPAMP,
)


