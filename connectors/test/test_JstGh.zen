load("../../properties.zen", "Layout")

JstGh = Module("../JstGh.zen")

# Generate all JST GH connector variants (2-14 pins, both orientations)
for pin_count in JstGh.PinCount.values():
    for orientation in JstGh.Orientation.values():
        # Create pin connections
        num_pins = int(pin_count.value)
        name = "JstGh_" + pin_count.value + "pin_" + orientation.value
        pins = {"Pin_{}".format(n): Net(name + "_Pin_{}".format(n)) for n in range(1, num_pins + 1)}
        pins["MountPin"] = Net(name + "_MountPin")

        # Create the component
        JstGh(name=name, pin_count=pin_count, orientation=orientation, **pins)
