"""Comprehensive test for FerriteBead component."""

load("../../properties.zen", "Layout")

FerriteBead = Module("../FerriteBead.zen")

# Test all Package values
for package in FerriteBead.Package.values():
    name = "FB_" + str(package)
    FerriteBead(
        name = name,
        resistance = "100Ohm",
        frequency = "100MHz",
        current = "500mA",
        package = package,
        P1 = Net(name + "_P1"),
        P2 = Net(name + "_P2"),
    )
