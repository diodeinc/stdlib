load(
    "../../interfaces.zen",
    "Analog",
    "Ground",
    "I2c",
    "OscPair",
    "Power",
    "Pwm",
    "Qspi",
    "Spi",
    "Swd",
    "Uart",
    "Usb2",
)
load(
    "../../pins.zen",
    "PinInterface",
    "PinMapBuilder",
    "Pins",
)

def PinMap(pins: Pins = {}):
    """RP2040 Pin Map

    Args:
        pins: Pins to assign to the pin map.

    Returns:
        Pin map.
    """

    OSC = PinInterface("OSC", OscPair)
    USB = PinInterface("USB", Usb2)
    QSPI = PinInterface("QSPI", Qspi)
    SWD = PinInterface("SWD", Swd)
    I2C0 = PinInterface("I2C0", I2c)
    I2C1 = PinInterface("I2C1", I2c)
    UART0 = PinInterface("UART0", Uart)
    UART1 = PinInterface("UART1", Uart)
    SPI0 = PinInterface("SPI0", Spi)
    SPI1 = PinInterface("SPI1", Spi)
    PWM0 = PinInterface("PWM0", Pwm)
    PWM1 = PinInterface("PWM1", Pwm)
    PWM2 = PinInterface("PWM2", Pwm)
    PWM3 = PinInterface("PWM3", Pwm)
    PWM4 = PinInterface("PWM4", Pwm)
    PWM5 = PinInterface("PWM5", Pwm)
    PWM6 = PinInterface("PWM6", Pwm)
    PWM7 = PinInterface("PWM7", Pwm)
    ADC0 = PinInterface("ADC0", Analog)
    ADC1 = PinInterface("ADC1", Analog)
    ADC2 = PinInterface("ADC2", Analog)
    ADC3 = PinInterface("ADC3", Analog)

    return PinMapBuilder(
        pin_nets = pins,
        pin_map = {
            "XIN": [OSC.XIN],
            "XOUT": [OSC.XOUT],
            "USB_DP": [USB.DP],
            "USB_DM": [USB.DM],
            "QSPI_SS_N": [QSPI.CS],
            "QSPI_SCLK": [QSPI.CLK],
            "QSPI_SD0": [QSPI.IO0],
            "QSPI_SD1": [QSPI.IO1],
            "QSPI_SD2": [QSPI.IO2],
            "QSPI_SD3": [QSPI.IO3],
            "SWDIO": [SWD.SWDIO],
            "SWCLK": [SWD.SWCLK],
            "GPIO0": [I2C0.SDA, UART0.TX, SPI0.MISO, PWM0.NET],
            "GPIO1": [I2C0.SCL, UART0.RX, SPI0.CS, PWM0.NET],
            "GPIO2": [I2C1.SDA, SPI0.CLK, PWM1.NET],
            "GPIO3": [I2C1.SCL, SPI0.MOSI, PWM1.NET],
            "GPIO4": [I2C0.SDA, UART1.TX, SPI0.MISO, PWM2.NET],
            "GPIO5": [I2C0.SCL, UART1.RX, SPI0.CS, PWM2.NET],
            "GPIO6": [I2C1.SDA, SPI0.CLK, PWM3.NET],
            "GPIO7": [I2C1.SCL, SPI0.MOSI, PWM3.NET],
            "GPIO8": [I2C0.SDA, UART1.TX, SPI1.MISO, PWM4.NET],
            "GPIO9": [I2C0.SCL, UART1.RX, SPI1.CS, PWM4.NET],
            "GPIO10": [I2C1.SDA, SPI1.CLK, PWM5.NET],
            "GPIO11": [I2C1.SCL, SPI1.MOSI, PWM5.NET],
            "GPIO12": [I2C0.SDA, UART0.TX, SPI1.MISO, PWM6.NET],
            "GPIO13": [I2C0.SCL, UART0.RX, SPI1.CS, PWM6.NET],
            "GPIO14": [I2C1.SDA, SPI1.CLK, PWM7.NET],
            "GPIO15": [I2C1.SCL, SPI1.MOSI, PWM7.NET],
            "GPIO16": [I2C0.SDA, UART0.TX, SPI0.MISO, PWM0.NET],
            "GPIO17": [I2C0.SCL, UART0.RX, SPI0.CS, PWM0.NET],
            "GPIO18": [I2C1.SDA, SPI0.CLK, PWM1.NET],
            "GPIO19": [I2C1.SCL, SPI0.MOSI, PWM1.NET],
            "GPIO20": [I2C0.SDA, UART1.TX, SPI0.MISO, PWM2.NET],
            "GPIO21": [I2C0.SCL, UART1.RX, SPI0.CS, PWM2.NET],
            "GPIO22": [I2C1.SDA, SPI0.CLK, PWM3.NET],
            "GPIO23": [I2C1.SCL, SPI0.MOSI, PWM3.NET],
            "GPIO24": [I2C0.SDA, UART1.TX, SPI1.MISO, PWM4.NET],
            "GPIO25": [I2C0.SCL, UART1.RX, SPI1.CS, PWM4.NET],
            "GPIO26/ADC0": [I2C1.SDA, SPI1.CLK, PWM5.NET, ADC0.NET],
            "GPIO27/ADC1": [I2C1.SCL, SPI1.MOSI, PWM5.NET, ADC1.NET],
            "GPIO28/ADC2": [I2C0.SDA, UART0.TX, SPI1.MISO, PWM6.NET, ADC2.NET],
            "GPIO29/ADC3": [I2C0.SCL, UART0.RX, SPI1.CS, PWM6.NET, ADC3.NET],
        },
    )

GPIO1 = Net("GPIO1")
i2c0 = I2c("I2C0")

pins = PinMap().connect("GPIO1", GPIO1).assign("I2C0", i2c0, SCL = "GPIO5", SDA = "GPIO4").build()
